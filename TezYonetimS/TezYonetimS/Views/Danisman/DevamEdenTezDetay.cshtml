        <li class="active"><a data-toggle="pill" href="#menu1">Detay</a></li>
        <li><a data-toggle="tab" href="#menu2">Yapılacak İşler</a></li>
        <li><a data-toggle="tab" href="#menu3">Toplantılar</a></li>
        <li><a data-toggle="tab" href="#menu4">Notlar</a></li>
    </ul>
             <br />
    @if (ViewBag.Uyari != null)
    {
        <div class="alert alert-danger" role="alert">
            @ViewBag.Uyari
        </div>

    }
    @if (TempData["Basarili"] != null)
    {
        <div class="alert alert-success" role="alert">
            @TempData["Basarili"]
        </div>

    }
    <div class="tab-content">
        <div id="menu1" class="tab-pane fade in active ">
          
            @Html.Partial("_TezDetayi",Model)
        </div>
        <div id="menu2" class="tab-pane fade">
         
            <p>Ut enim ad minim veniam, quis nostrud exercitation ullamco laboris nisi ut aliquip ex ea commodo consequat.</p>
        </div>
        <div id="menu3" class="tab-pane fade">
          
